{
    "manifest_version": 2,
    "name": "Coding Train Extension",
    "version": "0.2",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "ct.png",
        "default_popup": "popup.html",
        "default_title": "Auto Liking below"

    }
}